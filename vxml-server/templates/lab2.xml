<?xml version="1.0" encoding="UTF-8" ?>
<vxml version="2.0">
  <nomatch>
    I didn't understand.
  </nomatch>
  
  <noinput>
    You didn't say anything.
  </noinput>

  <form id ="funnyQuotes">
    <field name="say">
      <prompt>Say one of the silly phrases</prompt>
      <grammar src="grammars/lab2.xml:"/>
    </field>
    <filled>
    <prompt>
      You said: <value expr="say$.utterance"/>,
      with the interpretation <value expr="foo$.interpretation"/>
      and with a confidence of <value expr="foo$.confidence"/>
      <break time ="2s"/>
      </prompt>
      </form>

</vxml>
